{
    "name": "laravel/laravel",
    "description": "The Laravel Framework.",
    "keywords": ["framework", "laravel"],
    "license": "MIT",
    "type": "project",
    "require": {
        "php": ">=7.1.3",
        "barryvdh/laravel-cors": "^0.11.0",
        "cloudcreativity/laravel-stripe": "1.x-dev",
        "cviebrock/eloquent-sluggable": "^4.5",
        "doctrine/dbal": "^2.10",
        "elasticsearch/elasticsearch": "^6.0",
        "embed/embed": "^3.4",
        "fahim/laravel5-paypal-ipn": "^1.0",
        "fideloper/proxy": "^4.2",
        "konekt/pdf-invoice": "^1.5",
        "laravel/cashier": "^9.0",
        "laravel/framework": "5.8.*",
        "laravel/passport": "^7.0",
        "laravel/scout": "^7.0",
        "laravel/tinker": "^2.0",
        "maatwebsite/excel": "^3.1",
        "maddhatter/laravel-fullcalendar": "^1.3",
        "musonza/chat": "3.5.0",
        "nesbot/carbon": "^2.34",
        "nicolaslopezj/searchable": "^1.10",
        "plank/laravel-metable": "^2.0",
        "predis/predis": "^1.1",
        "pusher/pusher-php-server": "^3.0",
        "spatie/laravel-activitylog": "^3.0",
        "spatie/laravel-google-calendar": "^2.2",
        "spatie/laravel-medialibrary": "^7.0",
        "tolawho/loggy": "^1.0",
        "yab/laravel-scout-mysql-driver": "^3.0"
    },
    "require-dev": {
        "facade/ignition": "^2.0",
        "filp/whoops": "~2.0",
        "fzaninotto/faker": "^1.9.1",
        "laracasts/generators": "^1.1",
        "laravel/homestead": "^7.8",
        "mockery/mockery": "^1.3.1",
        "phpunit/phpunit": "^8.5",
        "nunomaduro/collision": "^4.1"
    },
    "autoload": {
        "classmap": [
            "database/seeds",
            "database/factories"
        ],
        "psr-4": {
            "App\\": "app/"
        },
        "files": [
            "app/helpers.php"
        ]
    },
    "autoload-dev": {
        "psr-4": {
            "Tests\\": "tests/"
        }
    },
    "extra": {
        "laravel": {
            "dont-discover": [
            ]
        }
    },
    "scripts": {
        "post-root-package-install": [
            "@php -r \"file_exists('.env') || copy('.env.example', '.env');\""
        ],
        "post-create-project-cmd": [
            "@php artisan key:generate"
        ],
        "post-autoload-dump": [
            "Illuminate\\Foundation\\ComposerScripts::postAutoloadDump",
            "@php artisan package:discover"
        ]
    },
    "config": {
        "preferred-install": "dist",
        "sort-packages": true,
        "optimize-autoloader": true
    }
}
